{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "useJSXTextNode": true,
    "project": "./tsconfig.json"
  },
  "extends": "standard",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-unused-expressions": "warn",
    "import/prefer-default-export": "off"
  },
  "env": {
    "node": true
  }
}
